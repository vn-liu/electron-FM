{
  "name": "electron-fm",
  "productName": "MAC-FM",
  "version": "1.0.0",
  "description": "ximalaya-FM",
  "main": "main.js",
  "scripts": {
    "electron": "wait-on http://localhost:5555 && NODE_ENV=development electron .",
    "start": "webpack-dev-server --inline --hot --progress",
    "electron-pack": "build --em.main=./main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a993630/electron-FM.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a993630/electron-FM/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "com.XMLYFM",
    "files": [
      "build/",
      "icon/",
      "main.js",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "icon": "icon/app-icon.icns",
      "target": "dmg"
    },
    "dmg": {
      "icon": "icon/app-icon.icns",
      "title": "MAC-FM",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "electron": "~1.7.8",
    "electron-builder": "^19.49.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "wait-on": "^2.0.2"
  }
}
